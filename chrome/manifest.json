{ "name": "Github improved"
, "version": "2.0.6"
, "manifest_version": 2
, "icons":
  { "128": "128.png"
  }
, "description": "Adds in-page changeset unfolding, and some other niceties for github's Commit History view."
, "content_scripts":
  [ { "js":      [ "unfold_commit_history.user.js"
                 ]
    , "matches": [ "https://*/*/commits*"
                 , "https://*/*/compare*"
                 , "https://*/*/search*"
                 ]
    , "run_at": "document_end"
    }
  ]
}
